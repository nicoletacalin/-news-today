<h1>All Posts</h1>
<% @posts.each do |post| %>
  <div class="panel panel-default">
    <div class="panel-body">
      <h4>
        <%= link_to post.title, post_path(post) %> |  <%= link_to "Edit", edit_post_path(post), method: :get %> |<%= link_to 'Delete', post_path(post), method: :delete,  data: { confirm: 'Are you sure?', remote: true } %>
      </h4>
    </div>
  </div>
<% end %>
